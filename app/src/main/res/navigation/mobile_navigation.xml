<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.starbook.ui.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/home_fragment" />

    <fragment
        android:id="@+id/navigation_list"
        android:name="com.example.starbook.ui.list.ListFragment"
        android:label="List"
        tools:layout="@layout/list_fragment" />

    <fragment
        android:id="@+id/navigation_search"
        android:name="com.example.starbook.ui.search.SearchFragment"
        android:label="Search"
        tools:layout="@layout/search_fragment" />

    <fragment
        android:id="@+id/navigation_account"
        android:name="com.example.starbook.ui.account.AccountFragment"
        android:label="Account"
        tools:layout="@layout/account_fragment"/>
</navigation>